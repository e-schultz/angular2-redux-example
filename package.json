    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "^0.33.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
