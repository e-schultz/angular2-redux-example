{
  "name": "angular2-redux-starter",
  "version": "0.2.0",
  "description": "A boiler-plate setup for Angular 2/Redux applications",
  "license": "MIT",
  "keywords": [
    "angular2",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings && typings install",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "npm run lint && karma start",
    "lint": "tslint './src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/angular2-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/angular2-redux-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^1.3.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.7",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-basic": "^1.0.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.4",
    "ng2-redux": "^2.0.2",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "ramda": "^0.19.1",
    "raw-loader": "^0.5.1",
    "redux": "^3.0.6",
    "redux-devtools": "^3.0.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^2.0.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.1",
    "rxjs": "5.0.0-beta.2",
    "selenium-standalone": "^4.8.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.6",
    "typings": "^0.6.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1",
    "zone.js": "0.5.15"
  }
}
