{
  "name": "angular2-redux-starter",
  "version": "0.2.0",
  "description": "A boiler-plate setup for Angular 2/Redux applications",
  "license": "MIT",
  "keywords": [
    "angular2",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings && typings install",
    "dev": "NODE_ENV=development nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/angular2-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/angular2-redux-starter/issues"
  },
