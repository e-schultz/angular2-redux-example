{
  "name": "angular2-redux-example",
  "version": "0.2.0",
  "description": "A boiler-plate setup for Angular 2/Redux applications",
  "license": "MIT",
  "keywords": [
    "angular2",
    "redux",
    "typescript",
    "ng2-redux"
  ],
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "cover": "remap-istanbul -i coverage/chrome/coverage-final.json -o coverage/coverage.json",
    "postcover": "istanbul report lcov text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --progress --no-info",
    "lint": "npm run lint-ts && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-ts": "tslint './src/**/*.ts'",
    "postinstall": "npm run typings && npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "rimraf coverage/ & npm run lint",
    "test": "karma start",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false --restartOnFileChange true",
    "typings": "rimraf typings/ && typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-schultz/angular2-redux-example.git"
  },
  "bugs": {
    "url": "https://github.com/e-schultz/angular2-redux-example/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^1.3.0",
    "http-proxy": "^1.13.2",
    "ng2-redux": "^2.2.2-beta.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.15",
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-align": "^1.0.2",
    "basscss-basic": "^1.0.0",
    "basscss-border": "^4.0.2",
    "basscss-flexbox": "^1.0.1",
    "basscss-grid": "^2.0.0",
    "basscss-hide": "^1.0.1",
    "basscss-layout": "^3.1.0",
    "basscss-margin": "^1.0.7",
    "basscss-padding": "^1.1.3",
    "basscss-position": "^2.0.3",
    "basscss-type-scale": "^1.0.5",
    "basscss-typography": "^3.0.3",
    "chai": "^3.5.0",
    "classlist-polyfill": "^1.0.2",
    "console-polyfill": "^0.2.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "html5-history-api": "^4.2.7",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.4",
    "ng2-redux": "^2.2.1-0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "raf": "^3.2.0",
    "ramda": "^0.21.0",
    "raw-loader": "^0.5.1",
    "redux": "^3.4.0",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.6.1",
    "rimraf": "^2.5.1",
    "rxjs": "5.0.0-beta.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "stylelint": "^5.4.0",
    "stylelint-webpack-plugin": "^0.1.0",
    "to-string-loader": "^1.1.3",
    "ts-loader": "^0.8.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.6",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1",
    "zone.js": "0.6.11"
  }
}
